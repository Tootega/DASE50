{
    "compilerOptions":  {
                            "target":  "ES2022",
                            "module":  "CommonJS",
                            "moduleResolution":  "node",
                            "lib":  [
                                        "ES2022",
                                        "DOM"
                                    ],
                            "outDir":  "./dist",
                            "rootDir":  "./src",
                            "declaration":  true,
                            "declarationMap":  true,
                            "sourceMap":  true,
                            "strict":  true,
                            "esModuleInterop":  true,
                            "skipLibCheck":  true
                        },
    "include":  [
                    "src/**/*.ts"
                ],
    "exclude":  [
                    "node_modules",
                    "dist"
                ]
}
